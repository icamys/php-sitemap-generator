{
  "name": "icamys/php-sitemap-generator",
  "description": "Simple PHP sitemap generator.",
  "keywords": ["php", "sitemap", "generator", "psr-1", "psr-2", "psr-4"],
  "homepage": "https://github.com/icamys/php-sitemap-generator",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Dmitrii Prisacari",
      "email": "prisacari.dmitrii@gmail.com",
      "homepage": "https://github.com/icamys/"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-SPL": "*",
    "ext-xml": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-mbstring": "*",
    "ext-zlib": "*",
    "ext-curl": "*",
    "ext-xmlwriter": "*"
  },
  "autoload": {
    "psr-4": {"Icamys\\SitemapGenerator\\": "src/"}
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.10",
    "php-mock/php-mock-phpunit": "^2.7.2",
    "vimeo/psalm": "^5.7"
  }
}
